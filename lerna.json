{
  "npmClient": "yarn",
  "useWorkspaces": true,
  "packages": [
    "./[!_]*"
  ],
  "command": {
    "version": {
      "exact": true
    },
    "publish": {
      "npmClient": "npm",
      "allowBranch": [
        "staging",
        "main",
        "canary"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  },
  "version": "0.1.1-alpha.0",
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "useNx": false
}